<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!--导入属性配置 -->
    <properties resource="generator.properties"></properties>
    <!-- 数据库驱动包位置 -->
    <classPathEntry location="lib/sqljdbc4-chs-4.0.2206.100.jar" />
    <context id="elitel" defaultModelType="flat">
        <jdbcConnection driverClass="${jdbc.driverClass}" connectionURL="${jdbc.connectionURL}" userId="${jdbc.userId}" password="${jdbc.password}" />
        <javaModelGenerator targetPackage="com.elitel.rain.yearfall.entity" targetProject="${generator.project.path}\src\main\java" />
        <sqlMapGenerator targetPackage="mybatis\hy010\mapper" targetProject="${generator.project.path}\src\main\resources" />
        <javaClientGenerator targetPackage="com.elitel.rain.yearfall.dao" targetProject="${generator.project.path}\src\main\java" type="XMLMAPPER" />
        <!--domainObjectName 属性为对应要生成的实体名称 不指定默认为表名-->
        <table tableName="A_DATA" domainObjectName="SqlServerAData" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>
